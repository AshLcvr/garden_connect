<div class="photo_profile">
    {% if user.image|length != NULL %}
        {% if user.id > 136  %}
            <img style="border-radius: 50%;border:5px solid white" src="{{ asset('uploads/profile/' ~ user.image) }}" alt="Photo de profil" />
        {% else %}
            <img style="border-radius: 50%;border:5px solid white" src="{{ user.image }}" alt="Photo de profil"  />
        {% endif %}
    {% else %}
        <img src="{{ asset('uploads/profile/incognito-62f7bceeb2bee.png') }}" alt="Photo de profil"  />
    {% endif %}
</div>